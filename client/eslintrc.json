{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "object-curly-newline": "off",
    "import/order": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "linebreak-style": 0
  }
}
